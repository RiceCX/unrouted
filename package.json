    "cookie": "latest",
    "cors": "^2.8.5",
    "destr": "latest",
    "eslint": "latest",
    "eslint-plugin-jest": "^25.3.0",
    "express": "latest",
    "get-port": "latest",
    "jest": "27.4.3",
    "jiti": "1.12.9",
    "koa": "^2.13.4",
    "listhen": "0.2.5",
    "release-it": "^14.11.8",
    "siroc": "latest",
